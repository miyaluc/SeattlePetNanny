@model SeattlePetNanny.Models.Owner
@{
    Layout = "_Layout";
}

<div class="jumbotron profileBanner text-center">
    <div class="bannerText">
        <a href="~/Home"><img class="proBanner" src="~/imgs/SPN LOGO .png" /></a>
        <h4 class="greeting">Profile</h4>
    </div>
</div>


<div class="petInfo">

</div>



<div class="container userInfo">

    <div class="userDisplay">

        <div class="proPic"></div>

        @*@Model.FirstName*@ 
</div>
</div>

<a class="btn" asp-action="Modal" asp-controller="Account" asp-route-id="1" data-target="#modal-container" data-toggle="modal">Add Pet</a>
<div id="modal-container" class="modal fade hidden-print" tabindex="-1" role="dialog">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class=" btn addPet">
            <button type="submit">Add New Pet</button>
            </div>
        </div>
    </div>
</div>



